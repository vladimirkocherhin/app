-
    type: content
    mode: create
    content_type: phrase
    parent_location: {{ parent_location|raw }}
    remote_id: 'rid:phrase{{ id }}'
    location_remote_id: 'lrid:phrase{{ id }}'
    lang: eng-GB
    attributes:
        title: {{ english }}
    {% if is_category %}
references:
        -
            identifier: phrase_{{ id }}
            attribute: location_id
        {% endif %}

